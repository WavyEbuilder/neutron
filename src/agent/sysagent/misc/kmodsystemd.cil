(in kmod

    (block unit

      (filecon "/usr/lib/systemd/system/kmod-static-nodes\.service.*" file
               file_context)
      (filecon "/usr/lib/systemd/system/modprobe@.*\.service.*" file file_context)

      (blockinherit .file.unit.template)))
